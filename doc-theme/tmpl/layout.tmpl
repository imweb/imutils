    <script>
     (function() {
       var VERSIONS = <?js=versions ?>;
       function renderVersions() {
         var el = document.getElementById('versions');
         var select = document.createElement('select');
         var selectedIndex = 0;
         var pathname = location.pathname;
         var versionOld;
         VERSIONS.forEach(function(v, i) {
           if (pathname.indexOf(v) > -1) {
             versionOld = v;
             selectedIndex = i;
           }
           select.options.add(new Option(v,v));
         });
         select.selectedIndex = selectedIndex;
         el.appendChild(select);
         select.addEventListener('change', function(e) {
           var versionNew = e.target.value;
           var pathname = location.pathname;
           window.location = pathname.replace(versionOld, versionNew);
           versionOld = versionNew;
         })
       }
       window.addEventListener('load', renderVersions, false);
     })();
    </script>
